{
  "name": "tms-api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "{projectRoot}"
      },
      "outputs": ["{projectRoot}/dist"],
      "configurations": {
        "production": {}
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run dev",
        "cwd": "{projectRoot}"
      },
      "configurations": {
        "production": {
          "command": "npm run start",
          "cwd": "{projectRoot}"
        }
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm test",
        "cwd": "{projectRoot}"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run lint",
        "cwd": "{projectRoot}"
      }
    },
    "seed-api": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run seed:api",
        "cwd": "{projectRoot}"
      },
      "description": "Seed sample shipments via GraphQL (server must be running)"
    }
  },
  "tags": ["scope:api", "type:app"]
}
